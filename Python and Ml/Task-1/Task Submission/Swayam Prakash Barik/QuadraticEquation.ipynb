   "source": [
    "import cmath\n",
    "a=int(input(\"Enter the co-efficient of x^2 :\"))\n",
    "b=int(input(\"Enter the co-efficient of x :\"))\n",
    "c=int(input(\"Enter the constant :\"))\n",
    "\n",
    "d=b*b-4*a*c\n",
    "ans1=(-b+cmath.sqrt(d))/(2*a)\n",
    "ans2=(-b-cmath.sqrt(d))/(2*a)\n",
    "print(f\"root1 = {ans1}\") \n",
    "print(f\"root2 = {ans2}\")"
   ]
  }
